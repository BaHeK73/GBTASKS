# Telephone Directory Program

# Create a dictionary to store data
directory = {}

# Function to add new entries
def add_entry():
    # Get name and phone number
    name = input("Name: ")
    phone = input("Phone Number: ")

    # Add entry to dictionary
    directory[name] = phone

# Function to look up an entry
def look_up():
    # Get name to look up
    name = input("Name: ")

    # Get phone number from dictionary
    phone = directory.get(name, "Not found")

    # Print result
    print("Phone number:", phone)

# Function to list all entries
def list_entries():
    # Print all entries in the directory
    for name, phone in directory.items():
        print(name, phone)

# Function to delete an entry
def delete_entry():
    # Get name to delete
    name = input("Name: ")

    # Delete entry from dictionary
    del directory[name]

# Main function
def main():
    # Print welcome message
    print("Welcome to")